{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceRoot}/src",
      "runtimeExecutable": "/usr/bin/chromium"
    },
    {
      "name": "Gatsby develop",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
      "args": [
        "develop"
      ],
      "stopOnEntry": false,
      "runtimeArgs": [
        "--nolazy"
      ],
      "sourceMaps": false,
      "console": "integratedTerminal"
    }
  ]
}